                    }
                });
                */
                
                $('#check_all_ct').live('click', function() {
                    status = $('#check_all_ct').attr("checked");
                    if(status == 'checked') {
                        $('.ct_opt').attr("checked", true);
                    }
                    else {
                        $('.ct_opt').attr("checked", false);
                    }
                });
                
                $('#check_all_mt').live('click', function() {
                    status = $('#check_all_mt').attr("checked");
                    if(status == 'checked') {
                        $('.mt_opt').attr("checked", true);
                    }
                    else {
                        $('.mt_opt').attr("checked", false);
                    }
                });
            });
        </script>
